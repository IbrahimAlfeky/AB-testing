    "# Fit your model, and summarize the results\n",
    "df11['ab_CA'] = df11['ab_page'] * df11['CA']\n",
    "df11['ab_UK'] = df11['ab_page'] * df11['UK']\n",
    "logit_mod = sm.Logit(df11['converted'], df11[['intercept', 'ab_page', 'CA','UK', 'ab_CA', 'ab_UK']])\n",
    "results = logit_mod.fit()\n",
    "results.summary()"
